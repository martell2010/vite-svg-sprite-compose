{
  "name": "vite-svg-sprite-compose",
  "version": "1.1.1",
  "description": "Vite SVG Sprite Merge Plugin is a Vite plugin that simplifies and automates the merging of multiple SVG icons into a single sprite. It allows you to combine SVG icons from various directories, apply optional SVGO optimization, and generate a unified SVG sprite with customizable configurations. Streamline your workflow by easily creating SVG sprite icons for use in web applications.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "release": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martell2010/vite-svg-sprite-compose.git"
  },
  "author": {
    "name": "Listviahov Serhii",
    "email": "listvagov@gmail.com",
    "url": "https://github.com/martell2010"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martell2010/vite-svg-sprite-compose/issues"
  },
  "homepage": "https://github.com/martell2010/vite-svg-sprite-compose#readme",
  "keywords": [
    "svgo",
    "svg",
    "optimize",
    "vite",
    "sprite",
    "combine",
    "merge"
  ],
  "peerDependencies": {
    "vite": "^4.5.0"
  },
  "dependencies": {
    "svg-sprite": "^3.0.0-beta2",
    "svgo": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/svg-sprite": "^0.0.36",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
